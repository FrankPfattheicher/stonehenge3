<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <id>IctBaden.Stonehenge3</id>
        <version>3.0.0</version>
        <authors>Frank Pfattheicher</authors>
        <owners>Frank Pfattheicher</owners>
        <summary>X-Platform Web Application Framework</summary>
        <license type="expression">MIT</license>
        <icon>images/icon64.png</icon>
        <projectUrl>https://github.com/FrankPfattheicher/stonehenge3</projectUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>X-Platform Web Application Framework</description>
        <releaseNotes>Stable</releaseNotes>
        <copyright>Copyright ICT Baden GmbH 2013-2021</copyright>
        <tags>.NET Core HTML CSS Kestrel Vue</tags>
        <dependencies>
            <dependency id="CryptSharpStandard" version="1.0.0" />
            <dependency id="HttpMultipartParser" version="4.3.1" />
            <dependency id="Microsoft.AspNetCore.Cors" version="2.2.0"/>
            <dependency id="Microsoft.AspNetCore.ResponseCompression" version="2.2.0"/>
            <dependency id="Microsoft.AspNetCore.Server.HttpSys" version="2.2.6" />
            <dependency id="Microsoft.AspNetCore.Server.IIS" version="2.2.6" />
            <dependency id="Microsoft.AspNetCore.Server.IISIntegration" version="2.2.1" />
            <dependency id="Microsoft.AspNetCore.Server.Kestrel" version="2.2.0"/>
            <dependency id="Microsoft.Extensions.Configuration" version="3.1.10"/>
            <dependency id="Microsoft.Extensions.Configuration.Abstractions" version="3.1.10"/>
            <dependency id="Microsoft.Extensions.DependencyInjection.Abstractions" version="3.1.10"/>
            <dependency id="Microsoft.Extensions.Http" version="3.1.10"/>
            <dependency id="Microsoft.Extensions.Hosting" version="3.1.10"/>
            <dependency id="Microsoft.Extensions.Hosting.Abstractions" version="3.1.10"/>
